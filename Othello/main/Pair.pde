class Pair {
  
  private int x;
  private int y;
 
  Pair (int x, int y){
    this.x=x;
    this.y=y;
  }
  
  public void setX (int x){
    this.x=x;
  }
  
  public void setY (int y){
    this.x=y;
  }
  
  public int getX() {
   return this.x; 
  }
  
  public int getY() {
   return this.y; 
  }
  
}
